{
  "comments": {
    "lineComment": {
      "comment": "//"
    },
    "blockComment": ["/*", "*/"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^(?:[;\\s]|\\/\\*(?:[^*]|\\*(?!\\/))*\\*\\/)*exports\\s*\\[",
      "end": "^(?:[^\\\\`\\n]|\\\\.)*`"
    }
  }
}

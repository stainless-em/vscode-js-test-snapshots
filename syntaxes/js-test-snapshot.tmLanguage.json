{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "JS Test Snapshot",
  "patterns": [
    {
      "include": "#value"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "begin": "/\\*",
          "beginCaptures": {
            "0": {
              "name": "punctuation.definition.comment.js"
            }
          },
          "end": "\\*/",
          "endCaptures": {
            "0": {
              "name": "punctuation.definition.comment.js"
            }
          },
          "name": "comment.block.js"
        },
        {
          "begin": "//",
          "beginCaptures": {
            "0": {
              "name": "comment.line.double-slash.js"
            }
          },
          "contentName": "comment.line.double-slash.js",
          "end": "(?=$)"
        }
      ]
    },
    "template": {
      "patterns": [
        {
          "begin": "`",
          "beginCaptures": {
            "0": {
              "name": "string.template.js punctuation.definition.string.template.begin.js"
            }
          },
          "contentName": "string.template.js",
          "end": "`",
          "endCaptures": {
            "0": {
              "name": "string.template.js punctuation.definition.string.template.end.js"
            }
          },
          "patterns": [
            {
              "include": "#string-character-escape"
            }
          ]
        }
      ]
    },

    "null-literal": {
      "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))null(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
      "name": "constant.language.null.js"
    },
    "numeric-literal": {
      "patterns": [
        {
          "captures": {
            "1": {
              "name": "storage.type.numeric.bigint.js"
            }
          },
          "match": "\\b(?<!\\$)0[Xx]\\h[_\\h]*(n)?\\b(?!\\$)",
          "name": "constant.numeric.hex.js"
        },
        {
          "captures": {
            "1": {
              "name": "storage.type.numeric.bigint.js"
            }
          },
          "match": "\\b(?<!\\$)0[Bb][01][01_]*(n)?\\b(?!\\$)",
          "name": "constant.numeric.binary.js"
        },
        {
          "captures": {
            "1": {
              "name": "storage.type.numeric.bigint.js"
            }
          },
          "match": "\\b(?<!\\$)0[Oo]?[0-7][0-7_]*(n)?\\b(?!\\$)",
          "name": "constant.numeric.octal.js"
        },
        {
          "captures": {
            "0": {
              "name": "constant.numeric.decimal.js"
            },
            "1": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "10": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "11": {
              "name": "storage.type.numeric.bigint.js"
            },
            "12": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "13": {
              "name": "storage.type.numeric.bigint.js"
            },
            "14": {
              "name": "storage.type.numeric.bigint.js"
            },
            "2": {
              "name": "storage.type.numeric.bigint.js"
            },
            "3": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "4": {
              "name": "storage.type.numeric.bigint.js"
            },
            "5": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "6": {
              "name": "storage.type.numeric.bigint.js"
            },
            "7": {
              "name": "storage.type.numeric.bigint.js"
            },
            "8": {
              "name": "meta.delimiter.decimal.period.js"
            },
            "9": {
              "name": "storage.type.numeric.bigint.js"
            }
          },
          "match": "(?<!\\$)(?:\\b[0-9][0-9_]*(\\.)[0-9][0-9_]*[Ee][-+]?[0-9][0-9_]*(n)?\\b|\\b[0-9][0-9_]*(\\.)[Ee][-+]?[0-9][0-9_]*(n)?\\b|\\B(\\.)[0-9][0-9_]*[Ee][-+]?[0-9][0-9_]*(n)?\\b|\\b[0-9][0-9_]*[Ee][-+]?[0-9][0-9_]*(n)?\\b|\\b[0-9][0-9_]*(\\.)[0-9][0-9_]*(n)?\\b|\\b[0-9][0-9_]*(\\.)(n)?\\B|\\B(\\.)[0-9][0-9_]*(n)?\\b|\\b[0-9][0-9_]*(n)?\\b(?!\\.))(?!\\$)"
        }
      ]
    },
    "numericConstant-literal": {
      "patterns": [
        {
          "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))NaN(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
          "name": "constant.language.nan.js"
        },
        {
          "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))Infinity(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
          "name": "constant.language.infinity.js"
        }
      ]
    },
    "undefined-literal": {
      "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))undefined(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
      "name": "constant.language.undefined.js"
    },
    "quoted-qstring-double": {
      "begin": "\"",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.js"
        }
      },
      "end": "\"",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.js"
        }
      },
      "name": "string.quoted.double.js",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    },
    "quoted-qstring-single": {
      "begin": "'",
      "beginCaptures": {
        "0": {
          "name": "punctuation.definition.string.begin.js"
        }
      },
      "end": "'",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.string.end.js"
        }
      },
      "name": "string.quoted.single.js",
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    },
    "key-string": {
      "match": "(?:((\")(?:[^\"\\\\]|\\\\.)*(\"))|((')(?:[^'\\\\]|\\\\.)*(')))\\s*(:)",
      "captures": {
        "1": {
          "name": "string.quoted.double.js support.type.property-name.json"
        },
        "2": {
          "name": "punctuation.definition.string.begin.js"
        },
        "3": {
          "name": "punctuation.definition.string.end.js"
        },
        "4": {
          "name": "string.quoted.single.js support.type.property-name.json"
        },
        "5": {
          "name": "punctuation.definition.string.begin.js"
        },
        "6": {
          "name": "punctuation.definition.string.end.js"
        },
        "7": {
          "name": "punctuation.separator.key-value.js"
        }
      },
      "patterns": [
        {
          "include": "#string-character-escape"
        }
      ]
    },
    "function": {
      "begin": "\\[Function\\s*",
      "beginCaptures": {
        "0": {
          "name": "entity.name.type.class.js meta.class.js"
        }
      },
      "end": "\\]",
      "endCaptures": {
        "0": {
          "name": "entity.name.type.class.js meta.class.js"
        }
      },
      "contentName": "entity.name.function.js"
    },
    "error": {
      "begin": "(\\[)((?:[$_[:alpha:]][$_[:alnum:]]*)?(?:Exception|Error|Warning)):\\s*",
      "end": "\\]",
      "name": "invalid.error"
    },
    "symbol": {
      "begin": "Symbol\\(",
      "end": "\\)",
      "name": "support.type.builtin.js"
    },
    "class": {
      "begin": "([$_[:alpha:]][$_[:alnum:]]*)\\s*({)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.type.class.js meta.class.js"
        },
        "2": {
          "name": "punctuation.definition.block.js"
        }
      },
      "end": "}",
      "endCaptures": {
        "0": {
          "name": "punctuation.definition.block.js"
        }
      },
      "patterns": [{ "include": "#rich-value" }]
    },
    "array-class": {
      "begin": "([$_[:alpha:]][$_[:alnum:]]*)\\s*(\\[)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.type.class.js meta.class.js"
        },
        "2": {
          "name": "meta.brace.square.js"
        }
      },
      "end": "\\]",
      "endCaptures": {
        "0": {
          "name": "meta.brace.square.js"
        }
      },
      "patterns": [{ "include": "#rich-value" }]
    },
    "square-class": {
      "begin": "\\[([$_[:alpha:]][$_[:alnum:]]*)(?!\\s*,)",
      "beginCaptures": {
        "0": {
          "name": "entity.name.type.class.js meta.class.js"
        }
      },
      "end": "\\]",
      "endCaptures": {
        "0": {
          "name": "entity.name.type.class.js meta.class.js"
        }
      },
      "patterns": [{ "include": "#rich-value" }]
    },
    "date": {
      "match": "(?:[+-][0-9]{2})?[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{3}Z",
      "name": "support.type.builtin.js"
    },
    "regex": {
      "match": "(\\/(?:(?:[^\\[/\\\\\\n\\r\\u2028\\u2029]|\\\\{2}(?:[$()*+.?\\]^{|}]|\\\\{2}|(?<=(?:[^\\\\]|^)(?:\\\\{4})+\\\\{2})\\[|\\[(?:(?:[^\\\\\\n\\r\\u2028\\u2029])|\\\\{2}(?:[$()*+.?\\[\\]^{|}]|\\\\{2}))*\\\\{2}\\])))*\\/)([dgimsuvy]*)",
      "captures": {
        "1": {
          "name": "string.regexp.js"
        },
        "2": {
          "name": "keyword.other.js"
        }
      }
    },
    "rich-template": {
      "patterns": [
        {
          "begin": "`",
          "beginCaptures": {
            "0": {
              "name": "string.template.js punctuation.definition.string.template.begin.js"
            }
          },
          "end": "`",
          "endCaptures": {
            "0": {
              "name": "string.template.js punctuation.definition.string.template.end.js"
            }
          },
          "patterns": [{ "include": "#rich-value" }]
        }
      ]
    },
    "rich-value": {
      "patterns": [
        {
          "include": "#date"
        },
        {
          "include": "#numeric-literal"
        },
        {
          "include": "#boolean-literal"
        },
        {
          "include": "#null-literal"
        },
        {
          "include": "#undefined-literal"
        },
        {
          "include": "#numericConstant-literal"
        },
        {
          "include": "#key-string"
        },
        {
          "include": "#quoted-string"
        },
        {
          "include": "#function"
        },
        {
          "include": "#symbol"
        },
        {
          "include": "#error"
        },
        {
          "include": "#regex"
        },
        {
          "include": "#class"
        },
        {
          "include": "#square-class"
        },
        {
          "include": "#array-class"
        },
        {
          "include": "#string-character-escape"
        }
      ]
    },
    "boolean-literal": {
      "patterns": [
        {
          "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))true(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
          "name": "constant.language.boolean.true.js"
        },
        {
          "match": "(?<![$_[:alnum:]])(?:(?<=\\.\\.\\.)|(?<!\\.))false(?![$_[:alnum:]])(?:(?=\\.\\.\\.)|(?!\\.))",
          "name": "constant.language.boolean.false.js"
        }
      ]
    },
    "string-character-escape": {
      "match": "\\\\(x\\h{2}|u\\h{4}|u\\{\\h+}|[012][0-7]{0,2}|3[0-6][0-7]?|37[0-7]?|[4-7][0-7]?|.|$)",
      "name": "constant.character.escape.js"
    },
    "quoted-string": {
      "patterns": [
        {
          "include": "#quoted-qstring-single"
        },
        {
          "include": "#quoted-qstring-double"
        }
      ]
    },
    "export": {
      "begin": "\\b(exports)\\s*(\\[)",
      "end": "\\]",
      "beginCaptures": {
        "1": {
          "name": "support.type.object.module.js"
        },
        "2": {
          "name": "meta.brace.square.js"
        }
      },
      "endCaptures": {
        "0": {
          "name": "meta.brace.square.js"
        }
      },
      "patterns": [
        {
          "include": "#template"
        },
        {
          "include": "#comments"
        }
      ]
    },
    "assign": {
      "match": "=",
      "name": "keyword.operator.assignment.js"
    },
    "semi": {
      "match": ";",
      "name": "punctuation.terminator.statement.js"
    },
    "value": {
      "patterns": [
        {
          "include": "#comments"
        },
        {
          "include": "#export"
        },
        {
          "include": "#assign"
        },
        {
          "include": "#rich-template"
        }
      ]
    }
  },
  "scopeName": "source.js.snapshot"
}
